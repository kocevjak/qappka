<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <RootNamespace>qappka_1._0_win</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <PackageIcon>water-drop.png</PackageIcon>
    <ApplicationIcon>water-drop.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="water-drop.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\water-drop.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.IO.Ports" Version="6.0.0" />
  </ItemGroup>

</Project>